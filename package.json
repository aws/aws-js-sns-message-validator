{
  "name": "sns-validator",
  "version": "0.3.5",
  "description": "A standalone validator for inbound SNS HTTP messages. No dependency on the AWS SDK for JavaScript.",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/aws-js-sns-message-validator.git"
  },
  "main": "index.js",
  "devDependencies": {
    "chai": "^3.3.0",
    "mocha": "^2.3.3",
